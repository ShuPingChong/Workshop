ition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-3 container-wrapper\"></div>\n            <div class=\"col-12 col-lg-9 container-wrapper\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">\n                        Our space</h2>\n                    <img src=\"@PROJECT_PATH@/assets/images/mbr-720x480.jpeg\" alt=\"Mobirise\" mbr-if=\"showImage_1\">\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 container-wrapper\">\n                <img src=\"@PROJECT_PATH@/assets/images/mbr-360x239.jpg\" alt=\"Mobirise\" mbr-if=\"showImage_2\">\n            </div>\n            <div class=\"col-12 col-lg-9 container-card\">\n                <div class=\"card-wrapper\">\n                    <p class=\"mbr-number mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".mbr-number\" mbr-if=\"showNumber\"><b>\n                        UNLIMITED</b><br><i><b>creat</b>ivity</i></p>\n                    <span mbr-ico